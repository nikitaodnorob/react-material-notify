{
  "extends": [
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "react/prop-types": "off"
  },
  "env": {
    "jest": true
  },
  "ignorePatterns": ["setupTests.ts"]
}
